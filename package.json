{
  "name": "discord-bot-seed",
  "version": "0.0.0",
  "private": true,
  "description": "Starter project for a discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && npm i && tsc",
    "compile": "tsc",
    "start": "node dist/src/index.js",
    "ts:watch": "tsc --watch",
    "dev": "nodemon dist/src/index.js",
    "prod": "node dist/src/index.js -- --production",
    "test": "jest",
    "test:watch": "jest --watch --coverage --coverage-reporters=lcov",
    "test:watchAll": "jest --watchAll --coverage --coverage-reporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/general_bradley/discord-bot-seed.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Bradley Mize",
  "license": "ISC",
  "homepage": "https://bitbucket.org/general_bradley/discord-bot-seed#readme",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-typescript": "^7.22.11",
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^28.1.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/node-cron": "^3.0.8",
    "@types/node-fetch": "^2.6.2",
    "babel-jest": "^29.6.4",
    "jest": "^28.1.1",
    "nodemon": "^2.0.18",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@discordjs/rest": "^2.0.1",
    "cheerio": "^1.0.0-rc.12",
    "discord-api-types": "^0.35.0",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "level": "^8.0.0",
    "minimist": "^1.2.6",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.8.1"
  }
}
