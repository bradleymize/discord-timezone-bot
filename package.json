{
  "name": "discord-bot-seed",
  "version": "0.0.0",
  "private": true,
  "description": "Starter project for a discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && npm i && tsc",
    "compile": "tsc",
    "start": "node dist/src/index.js",
    "ts:watch": "tsc --watch",
    "dev": "nodemon dist/src/index.js",
    "prod": "node dist/src/index.js -- --production",
    "test": "jest",
    "test:watch": "jest --watch --coverage --coverage-reporters=lcov",
    "test:watchAll": "jest --watchAll --coverage --coverage-reporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/general_bradley/discord-bot-seed.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Bradley Mize",
  "license": "ISC",
  "homepage": "https://bitbucket.org/general_bradley/discord-bot-seed#readme",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.14",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.15.19",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^3.0.3",
    "babel-jest": "^29.6.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@discordjs/rest": "^2.5.0",
    "cheerio": "^1.0.0",
    "discord-api-types": "^0.38.4",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "level": "^10.0.0",
    "minimist": "^1.2.8",
    "moment-timezone": "^0.5.48",
    "node-cron": "^4.0.3",
    "node-fetch": "^3.3.2",
    "winston": "^3.17.0"
  }
}
