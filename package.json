{
  "name": "discord-bot-seed",
  "version": "0.0.0",
  "private": true,
  "description": "Starter project for a discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && npm i && tsc",
    "compile": "tsc",
    "start": "node dist/src/index.js",
    "ts:watch": "tsc --watch",
    "dev": "nodemon dist/src/index.js",
    "prod": "node dist/src/index.js -- --production",
    "test": "jest",
    "test:watch": "jest --watch --coverage --coverage-reporters=lcov",
    "test:watchAll": "jest --watchAll --coverage --coverage-reporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/general_bradley/discord-bot-seed.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Bradley Mize",
  "license": "ISC",
  "homepage": "https://bitbucket.org/general_bradley/discord-bot-seed#readme",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.2",
    "@types/cheerio": "^0.22.34",
    "@types/jest": "^29.5.7",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.8.10",
    "@types/node-cron": "^3.0.10",
    "@types/node-fetch": "^3.0.3",
    "babel-jest": "^29.6.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@discordjs/rest": "^2.0.1",
    "cheerio": "^1.0.0-rc.12",
    "discord-api-types": "^0.37.62",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "level": "^8.0.0",
    "minimist": "^1.2.8",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.11.0"
  }
}
